{
    "manifest_version": 3,
    "name": "Google Docs",
    "version": "1.0",
    "description": "Edit, create, and view your documents without internet access.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "128": "128.png"
    }
  }
  